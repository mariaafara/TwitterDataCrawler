<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="950.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ViewTweetsController">
   <children>
      <ImageView fx:id="imageview" fitHeight="122.0" fitWidth="197.0" layoutX="65.0" layoutY="72.0" pickOnBounds="true" preserveRatio="true" />
      <Pane layoutX="109.0" layoutY="14.0" prefHeight="69.0" prefWidth="653.0">
         <children>
            <Button fx:id="choosebtn" layoutX="263.0" layoutY="22.0" mnemonicParsing="false" onAction="#chooseFile" prefHeight="25.0" prefWidth="171.0" text="Select a User file" />
         </children>
      </Pane>
      <Pane fx:id="pane" layoutX="295.0" layoutY="72.0" prefHeight="122.0" prefWidth="455.0" />
      <Label fx:id="name" layoutX="322.0" layoutY="105.0" prefHeight="17.0" prefWidth="306.0" />
      <Label fx:id="screen" layoutX="319.0" layoutY="131.0" prefHeight="17.0" prefWidth="387.0" />
      <Label fx:id="description" layoutX="323.0" layoutY="156.0" prefHeight="25.0" prefWidth="427.0" />
      <ScrollPane fx:id="scroll" layoutX="71.0" layoutY="203.0" prefHeight="368.0" prefWidth="810.0">
         <content>
            <VBox fx:id="vbox" prefHeight="364.0" prefWidth="806.0" />
