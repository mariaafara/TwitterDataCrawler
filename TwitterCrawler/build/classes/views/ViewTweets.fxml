      </ScrollPane>
      <Button fx:id="viewtweetshelp" layoutX="917.0" layoutY="576.0" mnemonicParsing="false" onAction="#openHelp" prefHeight="24.0" prefWidth="22.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView id="graphhelp" fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/icons8_Info_32px.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="loader" layoutX="794.0" layoutY="20.0" prefHeight="104.0" prefWidth="108.0" />
   </children>
</AnchorPane>
