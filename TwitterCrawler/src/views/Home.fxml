<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane id="MainBorderPane" fx:id="MainBorderPane" prefHeight="650.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HomeController">
   <top>
      <Pane fx:id="topPane" prefHeight="50.0" prefWidth="1100.0" style="-fx-background-color: #0f1e2d;" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="54.0" layoutY="18.0" text="TwitterDataCrawler" textFill="#fff7f7">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </top>
   <left>
      <VBox fx:id="sidePane" prefHeight="596.0" prefWidth="150.0" style="-fx-background-color: #0f1e2d;" stylesheets="@../styles/styling.css" BorderPane.alignment="CENTER">
         <children>
            <Button id="graphButton" fx:id="graphButton" mnemonicParsing="false" onMouseClicked="#show_graph" prefHeight="76.0" prefWidth="150.0" stylesheets="@../styles/styling.css" text="Graph">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button id="tweetsButton" fx:id="tweetsButton" mnemonicParsing="false" onMouseClicked="#show_tweets" prefHeight="75.0" prefWidth="150.0" stylesheets="@../styles/styling.css" text="Users">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button id="similartweetsButton" fx:id="viewTweets" mnemonicParsing="false" onMouseClicked="#show_viewTweets" prefHeight="77.0" prefWidth="150.0" stylesheets="@../styles/styling.css" text="View User">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button id="similartweetsButton" fx:id="similartweetsButton" mnemonicParsing="false" onMouseClicked="#show_similartweets" prefHeight="77.0" prefWidth="155.0" stylesheets="@../styles/styling.css" text="Similar Tweets">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </VBox>
   </left>
   <right>
      
   </right>
</BorderPane>
